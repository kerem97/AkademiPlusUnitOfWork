@model CustomerViewModel
@{
    Layout = null;
}
<h1>Unit Of Work Personel Prim Transferi</h1>

<br />
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <form method="post">
        <label>Gönderen</label>
        <input type="text" class="form-control" asp-for="SenderID" />
